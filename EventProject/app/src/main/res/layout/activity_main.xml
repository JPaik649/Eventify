<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/postButton3"
        android:layout_width="350dp"
        android:layout_height="150dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/postButton2" />

    <TextView
        android:id="@+id/postDateAndTime3"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton3"
        app:layout_constraintStart_toStartOf="@+id/postButton3"
        app:layout_constraintTop_toBottomOf="@+id/postLocation3" />

    <TextView
        android:id="@+id/postLocation3"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton3"
        app:layout_constraintStart_toStartOf="@+id/postButton3"
        app:layout_constraintTop_toBottomOf="@+id/postTag3" />

    <TextView
        android:id="@+id/postTag3"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton3"
        app:layout_constraintStart_toStartOf="@+id/postButton3"
        app:layout_constraintTop_toBottomOf="@+id/postTitle3" />

    <TextView
        android:id="@+id/postTitle3"
        android:layout_width="320dp"
        android:layout_height="29dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton3"
        app:layout_constraintStart_toStartOf="@+id/postButton3"
        app:layout_constraintTop_toTopOf="@+id/postButton3" />

    <Button
        android:id="@+id/postButton2"
        android:layout_width="350dp"
        android:layout_height="150dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/postButton1" />

    <TextView
        android:id="@+id/postDateAndTime2"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton2"
        app:layout_constraintStart_toStartOf="@+id/postButton2"
        app:layout_constraintTop_toBottomOf="@+id/postLocation2" />

    <TextView
        android:id="@+id/postLocation2"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton2"
        app:layout_constraintStart_toStartOf="@+id/postButton2"
        app:layout_constraintTop_toBottomOf="@+id/postTag2" />

    <TextView
        android:id="@+id/postTag2"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton2"
        app:layout_constraintStart_toStartOf="@+id/postButton2"
        app:layout_constraintTop_toBottomOf="@+id/postTitle2" />

    <TextView
        android:id="@+id/postTitle2"
        android:layout_width="320dp"
        android:layout_height="29dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton2"
        app:layout_constraintStart_toStartOf="@+id/postButton2"
        app:layout_constraintTop_toTopOf="@+id/postButton2" />

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </com.google.android.material.appbar.AppBarLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/fab_margin"
        android:layout_marginLeft="@dimen/fab_margin"
        android:layout_marginTop="@dimen/fab_margin"
        android:layout_marginEnd="48dp"
        android:layout_marginRight="48dp"
        android:layout_marginBottom="38dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/appBarLayout4"
        app:layout_constraintRight_toRightOf="@+id/appBarLayout4" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:popupTheme="@style/AppTheme.PopupOverlay" />

    <Button
        android:id="@+id/postButton1"
        android:layout_width="350dp"
        android:layout_height="150dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/postDateAndTime1"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton1"
        app:layout_constraintStart_toStartOf="@+id/postButton1"
        app:layout_constraintTop_toBottomOf="@+id/postLocation1" />

    <TextView
        android:id="@+id/postLocation1"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/postButton1"
        app:layout_constraintEnd_toEndOf="@+id/postButton1"
        app:layout_constraintStart_toStartOf="@+id/postButton1"
        app:layout_constraintTop_toBottomOf="@+id/postTag1"
        app:layout_constraintVertical_bias="0.062" />

    <TextView
        android:id="@+id/postTag1"
        android:layout_width="320dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton1"
        app:layout_constraintStart_toStartOf="@+id/postButton1"
        app:layout_constraintTop_toBottomOf="@+id/postTitle1" />

    <TextView
        android:id="@+id/postTitle1"
        android:layout_width="320dp"
        android:layout_height="29dp"
        android:layout_marginTop="10dp"
        android:elevation="10dp"
        android:text="textView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/postButton1"
        app:layout_constraintStart_toStartOf="@+id/postButton1"
        app:layout_constraintTop_toTopOf="@+id/postButton1" />

    <Button
        android:id="@+id/nextButton"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginBottom="36dp"
        android:background="@color/colorPrimary"
        android:drawableRight="@drawable/ic_forward_arrow"
        android:text=" Next"
        android:textColor="@color/gray"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/backButton" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginBottom="36dp"
        android:background="@color/colorPrimary"
        android:drawableLeft="@drawable/ic_back_arrow"
        android:text="Back "
        android:textColor="@color/gray"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>